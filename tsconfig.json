{
  "compilerOptions": {
    "target": "esnext",
    "esModuleInterop": true,
    "strict": true,
    "rootDir": "src",
    "outDir": "bots",
    "plugins": [],
    "typeRoots": [
      "node_modules/@types",
      "types"
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false,
    "moduleResolution": "Node"
  }
}
